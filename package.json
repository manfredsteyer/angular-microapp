{
  "name": "shell",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "live-server dist/shell",
    "build": "npm run build:shell && npm run build:clients",
    "build:clients": "npm run build:client-a && npm run build:client-b",
    "build:client-a": "ng build --prod --single-bundle --project client-a --output-hashing none --vendor-chunk false --output-path dist/shell/client-a",
    "build:client-b": "ng build --prod --single-bundle --project client-b --output-hashing none --vendor-chunk false --output-path dist/shell/client-b",
    "build:shell": "ng build --project shell",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.1",
    "@angular/common": "~8.0.1",
    "@angular/compiler": "~8.0.1",
    "@angular/core": "~8.0.1",
    "@angular/elements": "~8.0.1",
    "@angular/forms": "~8.0.1",
    "@angular/platform-browser": "~8.0.1",
    "@angular/platform-browser-dynamic": "~8.0.1",
    "@angular/router": "~8.0.1",
    "@webcomponents/custom-elements": "^1.1.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.4",
    "ngx-build-plus": "^8.0.3",
    "rxjs": "6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.800.3",
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "^8.0.3",
    "@angular/compiler-cli": "~8.0.1",
    "@angular/language-service": "~8.0.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "cpr": "^3.0.1",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "live-server": "^1.2.0",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.4.5",
    "webpack-cli": "^2.0.14"
  }
}
